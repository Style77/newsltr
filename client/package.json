{
  "name": "client",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json|css)\"",
    "check-format": "prettier --ignore-path .gitignore --check \"**/*.+(js|jsx|ts|tsx|json|css)\"",
    "typecheck": "tsc -b .",
    "test": "vitest",
    "cypress:open": "cypress open",
    "prepare": "cd .. && husky install client/.husky"
  },
  "dependencies": {
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "latest",
    "cypress": "^13.3.0",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "postcss": "latest",
    "prettier": "^3.0.3",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vitest": "^0.34.5"
  }
}
